    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.5.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
