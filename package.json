{
  "name": "minimal-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "cp ./node_modules/react/umd/react.development.js dist/react.development.js && cp ./node_modules/react-dom/umd/react-dom.development.js dist/react-dom.development.js",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "prettier": "prettier 'src/**/*.{ts,tsx}'",
    "prettier:ci": "npm run prettier -- -l",
    "prettier:fix": "npm run prettier -- --write",
    "tslint": "tslint -c tslint.json 'src/**/*.ts*'",
    "tslint:ci": "npm run tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [],
